<p id="notice"><%= notice %></p>

<h1>Listing Res Boards</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Address</th>
      <th>Phonenumber</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @res_boards.each do |res_board| %>
      <tr>
        <td><%= res_board.title %></td>
        <td><%= res_board.address %></td>
        <td><%= res_board.phoneNumber %></td>
        <td><%= link_to 'Show', res_board %></td>
        <td><%= link_to 'Edit', edit_res_board_path(res_board) %></td>
        <td><%= link_to 'Destroy', res_board, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Res board', new_res_board_path %>
